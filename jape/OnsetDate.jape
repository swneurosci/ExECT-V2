Rule: onsetDate6
Priority: 98

(   
	{Lookup2.majorType == historic, Lookup2.language == onset}
	{Lookup3.majorType == umls , Lookup3.Negation == Affirmed , Lookup3.Experiencer == Patient}
	{Lookup2.language == onset}
	{timeElapsed.time == in}
	{Numeric}
	
	
	
 ):match  
 -->
:match.OnsetDate = {rule = onsetDate6, OnsetType = "date_of_onset", 
PREF = :match.Lookup3.PREF, CUI = :match.Lookup3.CUI, TUI = :match.Lookup3.TUI,
STY = :match.Lookup3.STY, 
Year = :match.Numeric.value,
Month ="",
