Rule: onsetDate5
Priority: 98

(   
	{Lookup2.majorType == historic}
	{Lookup3.majorType == umls , Lookup3.Negation == Affirmed}
	{Lookup2.language == onset }
	{timeElapsed}
	{PartialDate}
	
	
	
 ):match  
 -->
:match.OnsetDate = {rule = onsetDate5, OnsetType = "date_of_onset", 
