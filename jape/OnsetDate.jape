Phase: OnsetDate
Input: Lookup3 Person Lookup2 Sentence Numeric timeElapsed  PointInTime Lookup PartialDate
Options: control=appelt


Rule: onsetDate1
Priority: 98

(   
	{Person}
	({Lookup2.majorType == historic})
	({Person})?
	{Lookup3.majorType == umls , Lookup3.Negation == Affirmed}
	{timeElapsed}
	{PartialDate}
	{PointInTime}
	
	
 ):match  
 -->
:match.OnsetDate = {rule = onsetDate1, OnsetType = "date_of_onset", 
PREF = :match.Lookup3.PREF, CUI = :match.Lookup3.CUI, TUI = :match.Lookup3.TUI,
STY = :match.Lookup3.STY, 
Year = "",
Month =:match.PartialDate.month,
Day = "",
PointInTime =:match.PointInTime.Since}

Rule: onsetDate2
Priority: 98

(   
	{Person}
    ({Lookup2})?
	{Lookup2.majorType == historic, Lookup2.language == onset}
	{Lookup3.majorType == umls , Lookup3.Negation == Affirmed}
	{timeElapsed.time == during}
	{PartialDate}
	
	
 ):match  
 -->
:match.OnsetDate = {rule = onsetDate2, OnsetType = "date_of_onset", 
PREF = :match.Lookup3.PREF, CUI = :match.Lookup3.CUI, TUI = :match.Lookup3.TUI,
STY = :match.Lookup3.STY, 
Year = :match.PartialDate.year,
Month = "",
Day = "",
PointInTime = ""} 

